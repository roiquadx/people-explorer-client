        "@types/node": "^22.0.0",
        "@vitejs/plugin-vue": "^6.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-vue": "^10.0.0",
        "prettier": "^3.0.0",
        "sass": "^1.80.0",
        "typescript": "^5.6.0",
        "typescript-eslint": "^8.0.0",
        "vite": "^7.0.0",
        "vue-tsc": "^2.0.0"
    }
}
